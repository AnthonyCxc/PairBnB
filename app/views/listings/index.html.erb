<h1>Listings#index</h1>
<p>Clearance Test Listing</p>
<%= render partial: "layouts/navigation" %>

<h1>Listings</h1>
  <table>
  	<thead>
  		<tr>
        <th>ID</th>
  			<th>Title</th>
  			<th>Description</th>
  			<th>Country</th>
  			<th>State/province</th>
  			<th>Address</th>
  			<th></th>
  			<th></th>
  			<th></th>
  		</tr>
  	</thead>

  	<tbody>
  		<% @listings.each do |listing|%>
  		<tr>
        <td><%= listing.id %></td>
  			<td><%= link_to listing.title,listing%></td>
  			<td><%= listing.description %></td>
  			<td><%= listing.country %></td>
  			<td><%= listing.state_or_province %></td>
  			<td><%= listing.address %></td>
  			<td><%= link_to'show',listing %></td>
  			<td><%# %></td>
  			<td><%# %></td>
  		</tr>
  		<% end %>
  	</tbody>
  </table>

  
  <%= link_to "Prev",listings_path(:page => @prev_page) %>
  <%= link_to "Next",listings_path(:page => @next_page) %>
  
