<%= form_for @listing do |f| %>
<div>
  <%= f.label :title %>
  <%= f.text_field :title %>
</div>
<div>
  <%= f.label :description %>
  <%= f.text_area :description %>
</div>
<div>
  <%= f.label :country %>
  <%= f.text_area :country %>
</div>
<div>
  <%= f.label :state_or_province %>
  <%= f.text_area :state_or_province %>
</div>
<div>
  <%= f.label :address %>
  <%= f.text_area :address %>
</div>
<div>
  <%= f.label :number_of_guests %>
  <%= f.number_field :number_of_guests %>
</div>
<div>
  <%= f.label :price_per_night %>
  <%= f.number_field :price_per_night %>
</div>
<div>
  <%= f.label :photos %>
  <%= f.file_field :photos, multiple: true %>
</div>

<%= f.submit "submit" %>
<%= @error if @error %>
<% end %>